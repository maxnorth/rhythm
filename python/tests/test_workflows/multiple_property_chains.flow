workflow(ctx, inputs) {
  let user = await Task.run("create_user", {name: "Bob", age: 25})
  let metadata = await Task.run("get_metadata", {})
  let result = await Task.run("combine_data", {
    name: user.data.name,
    age: user.data.age,
    timestamp: metadata.info.timestamp,
    version: metadata.info.version
  })
  return result
}
